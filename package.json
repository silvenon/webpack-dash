    "fs-promise": "^2.0.0",
    "lodash": "^4.17.15",
    "sequelize": "^3.26.0",
    "shelljs": "^0.8.3",
    "sqlite3": "^4.1.1"
  },
  "devDependencies": {
    "husky": "^3.0.9",
    "lint-staged": "^10.0.8",
    "prettier": "^1.19.1"
  }
}
