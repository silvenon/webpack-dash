{
  "private": true,
  "engines": {
    "node": ">=10 <=11"
  },
  "scripts": {
    "clean": "del webpack.js.org webpack.docset/Contents/Resources webpack.tgz",
    "build:frontend": "git clone https://github.com/webpack/webpack.js.org --depth 1 --branch gh-pages",
    "build:docset": "node .",
    "build": "yarn clean && yarn build:frontend && yarn build:docset"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix"
    ]
  },
  "dependencies": {
    "chalk": "^2.4.2",
    "cheerio": "^0.22.0",
    "del-cli": "^3.0.0",
    "fs-promise": "^2.0.0",
    "lodash": "^4.17.15",
    "sequelize": "^3.26.0",
    "shelljs": "^0.8.3",
    "sqlite3": "^5.0.0"
  },
  "devDependencies": {
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-prettier": "^3.1.2",
    "husky": "^3.0.9",
    "lint-staged": "^10.0.8",
    "prettier": "^1.19.1"
  }
}
